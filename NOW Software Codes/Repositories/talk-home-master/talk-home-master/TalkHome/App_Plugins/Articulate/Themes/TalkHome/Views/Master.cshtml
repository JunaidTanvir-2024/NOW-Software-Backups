@using ClientDependency.Core.Mvc
@using Articulate;
@model Articulate.Models.IMasterModel

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <title>@Model.PageTitle</title>
    @Html.MetaTags(Model)
    @Html.SocialMetaTags(Model)
    @Html.AdvertiseWeblogApi(Model)
    @Html.RssFeed(Model)
    @Html.RenderOpenSearch(Model)
    <meta name="HandheldFriendly" content="True" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="robots" content="index, follow">

    @{ Html
                      .RequiresThemedCssFolder(Model)
                      .RequiresThemedJsFolder(Model);
    }

    @Html.RenderCssHere()
    @if (!HttpContext.Current.IsDebuggingEnabled)
    {
        Html.RenderPartial("_MarketingHeaderTags");
    }
</head>
<body>
    @if (!HttpContext.Current.IsDebuggingEnabled)
    {
        <!-- Google Tag Manager (noscript) -->
        <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-PWJKCC7" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
        <!-- End Google Tag Manager (noscript) -->
    }
    @RenderBody()
    <script src="https://code.jquery.com/jquery-3.2.0.min.js" integrity="sha256-JAW99MJVpJBGcbzEuXk4Az05s/XyDdBomFqNlM3ic+I=" crossorigin="anonymous"></script>
    @Html.RenderJsHere()
    @Html.GoogleAnalyticsTracking(Model)

</body>

</html>
